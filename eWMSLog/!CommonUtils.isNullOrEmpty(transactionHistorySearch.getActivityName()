SQLQueryImpl( SELECT   distinct yardth.UPDATE_TS AS TransactionTimeStamp , acct.ACCT_NUMBER AS WhseID , whs.WHSE_NAME AS WhseName , funct.FUNCTN_NAME AS FuncSysName , COALESCE(userMgt.USER_FIRST_NAME,'INITIAL') || ' ' || COALESCE(userMgt.USER_LAST_NAME,'LOAD') AS CreatedBy , actvty.ACTVTY_NAME AS ActivityName , subact.SUB_ACTVTY_NAME AS TaskType , drivr.DRIVER_NUMBER AS DRIVER_NUMBER , COALESCE(drivr.DRIVER_FIRST_NAME,'INITIAL') || ' ' || COALESCE(drivr.DRIVER_LAST_NAME,'LOAD') AS UserID , ' 000 ' AS VehicleId , (Case When gateCheckIn.LOC_TYPE_CODE is not null Then t.TRAILR_NUMBER  Else '' End) AS TrailerNumber , ga.APPT_NUMBER AS ApptNumber , ga.TRACTR_NUMBER AS TractorNumber , ga.LEGACY_CARR_NAME || '-' || ga.LEGACY_CARR_NUMBER AS CarrierName , '000' AS FromLocation , gateCheckIn.LOC_TYPE_CODE AS ToLocation , '0000' AS SealNumber , '00000' AS BadgeNumber , dataTrailer.ACTUAL_TEMP AS ACTUALTEMP , dataTrailer.FUEL_LEVEL AS FUELLEVEL , dataTrailer.SET_TEMP AS SETTEMP , dataTrailer.CONDITION AS CONDITION FROM EWMUG1DBF.WHSE whs , EWMUG1DBF.SUB_ACTVTY subact , EWMUG1DBF.YARD_TRNSCT yardth left outer join EWMUG1DBF.User userMgt on yardth.USERID = userMgt.USERID , EWMUG1DBF.ACTVTY actvty , EWMUG1DBF.FUNCTN funct , EWMUG1DBF.DRIVER drivr , EWMUG1DBF.ACCT_TYPE acct , EWMUG1DBF.GATE_ARR ga left outer join EWMUG1DBF.TRAILR t on ga.TRAILR_SYSID = t.TRAILR_SYSID left outer join EWMUG1DBF.GATE_CHECK_IN gateCheckIn on ga.GATE_ARR_SYSID = gateCheckIn.GATE_ARR_SYSID left outer join EWMUG1DBF.YARD_TASK yt on (ga.TRAILR_SYSID = yt.TRAILR_SYSID and gateCheckIn.YARD_TASK_SYSID = yt.YARD_TASK_SYSID ) left outer join ( select yt.YARD_TASK_SYSID, CASE WHEN yt.TEMP_OFF_FLAG='Y' THEN 1000 ELSE yt.ACTUAL_TEMP END AS ACTUAL_TEMP, CASE WHEN yt.TEMP_OFF_FLAG='Y' THEN 1000 ELSE yt.SET_TEMP END AS SET_TEMP, yt.FUEL_LEVEL AS FUEL_LEVEL, STRIP(CASE WHEN  yt.NO_ISSUE_FLAG='Y' THEN 'No Issues' ELSE (CASE WHEN  yt.DAMAGE_TRAILR_FLAG='Y' THEN 'DT,' ELSE '' END || CASE WHEN  yt.REEFER_ISSUE_FLAG='Y' THEN 'RI,' ELSE '' END || CASE WHEN  yt.FLAT_TIRES_FLAG='Y' THEN 'FT,' ELSE '' END || CASE WHEN  yt.BROKEN_SEAL_FLAG='Y' THEN 'BS,' ELSE '' END) END,t,',') AS CONDITION from EWMUG1DBF.YARD_TASK yt ) dataTrailer on yt.YARD_TASK_SYSID=dataTrailer.YARD_TASK_SYSID where actvty.ACTVTY_SYSID=4  AND  funct.FUNCTN_SYSID=18  AND  ga.WHSE_SYSID=whs.WHSE_SYSID  AND  subact.SUB_ACTVTY_SYSID=yardth.SUB_ACTVTY_SYSID  AND  acct.ACCT_SYSID=whs.WHSE_SYSID  AND  subact.ACTVTY_SYSID=actvty.ACTVTY_SYSID  AND  drivr.DRIVER_NUMBER=ga.DRIVER_NUMBER  AND  ga.GATE_ARR_SYSID=yardth.GATE_ARR_SYSID  AND  whs.WHSE_SYSID=3  AND  yardth.UPDATE_TS BETWEEN TO_DATE('05/01/2015 00:00' , 'MM/dd/YYYY hh24:mi')   AND  TO_DATE('06/08/2015 23:59', 'MM/dd/YYYY hh24:mi') UNION ALL SELECT   distinct yardth.UPDATE_TS AS TransactionTimeStamp , acct.ACCT_NUMBER AS WhseID , whs.WHSE_NAME AS WhseName , funct.FUNCTN_NAME AS FuncSysName , COALESCE(userMgt.USER_FIRST_NAME,'INITIAL') || ' ' || COALESCE(userMgt.USER_LAST_NAME,'LOAD') AS CreatedBy , actvty.ACTVTY_NAME AS ActivityName , subact.SUB_ACTVTY_NAME AS TaskType , drivr.DRIVER_NUMBER AS DRIVER_NUMBER , COALESCE(drivr.DRIVER_FIRST_NAME,'INITIAL') || ' ' || COALESCE(drivr.DRIVER_LAST_NAME,'LOAD') AS UserID , ' 000 ' AS VehicleId , t.TRAILR_NUMBER AS TrailerNumber , ga.APPT_NUMBER AS ApptNumber , ga.TRACTR_NUMBER AS TractorNumber , ga.LEGACY_CARR_NAME || '-' || ga.LEGACY_CARR_NUMBER AS CarrierName , ga.LOC_TYPE_CODE AS FromLocation , '000' AS ToLocation , ga.SEAL_NUMBER AS SealNumber , '00000' AS BadgeNumber, CAST(null as CHAR) AS ACTUALTEMP, CAST(null as DECIMAL) AS FUELLEVEL, CAST(null as CHAR) AS SETTEMP, CAST(null as CHAR) AS CONDITION FROM EWMUG1DBF.WHSE whs , EWMUG1DBF.SUB_ACTVTY subact , EWMUG1DBF.YARD_TRNSCT yardth left outer join EWMUG1DBF.User userMgt on yardth.USERID = userMgt.USERID , EWMUG1DBF.ACTVTY actvty , EWMUG1DBF.FUNCTN funct , EWMUG1DBF.GATE_DEPART ga left outer join EWMUG1DBF.TRAILR t on ga.TRAILR_SYSID = t.TRAILR_SYSID , EWMUG1DBF.DRIVER drivr , EWMUG1DBF.ACCT_TYPE acct where actvty.ACTVTY_SYSID=4  AND  funct.FUNCTN_SYSID=18  AND  subact.SUB_ACTVTY_SYSID=yardth.SUB_ACTVTY_SYSID  AND  acct.ACCT_SYSID=whs.WHSE_SYSID  AND  ga.GATE_DEPART_SYSID=yardth.GATE_DEPART_SYSID  AND  subact.ACTVTY_SYSID=actvty.ACTVTY_SYSID  AND  drivr.DRIVER_NUMBER=ga.DRIVER_NUMBER  AND  ga.WHSE_SYSID=whs.WHSE_SYSID  AND  whs.WHSE_SYSID=3  AND  yardth.UPDATE_TS BETWEEN TO_DATE('05/01/2015 00:00' , 'MM/dd/YYYY hh24:mi')   AND  TO_DATE('06/08/2015 23:59', 'MM/dd/YYYY hh24:mi')  order by TransactionTimeStamp DESC)